{
  "version": 2,
  "builds": [
    {
      "src": "./api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./Frontend/vite.config.js",
      "use": "@vercel/static-build",
      "options": {
        "output": "Frontend/dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/Frontend/dist/index.html"
    }
  ]
}
